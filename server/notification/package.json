{
  "name": "notification-server",
  "version": "1.0.0",
  "description": "NestJS cron notification server",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:dev": "./node_modules/.bin/nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "proto:generate": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./src/proto-generated --ts_proto_opt=nestJs=true --proto_path=./src/proto ./src/proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.13.1",
    "@grpc/proto-loader": "^0.7.13",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.4.15",
    "@nestjs/mongoose": "^11.0.2",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "^11.0.7",
    "@nestjs/websockets": "^11.0.12",
    "@swc/cli": "^0.6.0",
    "axios": "^1.6.7",
    "firebase-admin": "^13.2.0",
    "install": "^0.13.0",
    "mongoose": "^8.12.2",
    "npm": "^11.2.0",
    "reflect-metadata": "^0.1.13",
    "run-script-webpack-plugin": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.1",
    "webpack": "^5.98.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "swc": "^1.0.11",
    "swc-loader": "^0.2.6",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "ts-proto": "^1.156.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}
